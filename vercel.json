{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/validar.php",  "dest": "/api/validar.php" },
      { "src": "/(.*)",  "dest": "/api/abrir_chamado.php" },
      { "src": "/(.*)",  "dest": "/api/area_cliente.php" },
      { "src": "/(.*)",  "dest": "/api/chamado_post.php" },
      { "src": "/(.*)",  "dest": "/api/liberar_sessao.php" },
      { "src": "/(.*)",  "dest": "/api/logoff.php" },
      { "src": "/(.*)",  "dest": "/api/consultar_chamado.php" }
    ]
  }